{
  "name": "mossaic-web-starter",
  "license": "ISC",
  "author": "S.Shchedromirskii",
  "repository": "https://github.com/mossaic-ui/web-widget-starter/mossaic-ui/web-widget-starter",
  "scripts": {
    "start": "yarn serve",
    "serve": "rimraf dist && SERVE='true' && NODE_ENV='production' && node esbuild.mjs",
    "serve:uikit": "UIKIT_MODE='true' && yarn serve",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "rimraf dist && NODE_ENV='production' && node esbuild.mjs",
    "lint-css": "stylelint",
    "lint:fix": "eslint ./(src|lib|packages)/**/**.(ts|html|js) --quiet --fix",
    "lint": "eslint ./(src|lib)/**/**.(ts|html|js) --quiet",
    "pre-commit": "yarn lint-staged",
    "prepack": "husky install",
    "prepare": "husky install",
    "postinstall": "husky install"
  },
  "engineStrict": true,
  "npmClient": "yarn",
  "packageManager": "yarn@3.5.0",
  "dependencies": {
    "date-fns": "^2.30.0",
    "lodash-es": "^4.17.21",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@html-eslint/eslint-plugin": "^0.17.2",
    "@html-eslint/parser": "^0.17.1",
    "@types/lodash-es": "^4",
    "@types/prettier": "^2",
    "@types/uuid": "^9",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "esbuild": "^0.18.6",
    "esbuild-plugin-svg": "^0.1.0",
    "esbuild-sass-plugin": "^2.10.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "postcss-modules": "^6.0.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "stylelint": "^15.8.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-declaration-strict-value": "^1.9.2",
    "stylelint-scss": "^5.0.1",
    "typescript": "4.8.4"
  }
}
